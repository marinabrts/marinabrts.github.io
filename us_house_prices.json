{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",

  "description": "Median Sales Price of Houses Sold for the United States",

   "title": {
    "text": "Median Price of Houses Sold for the United States",
    "subtitle":"Source: FRED",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "black"
  },
  
  "data": {
    "url": "https://raw.githubusercontent.com/marinabrts/marinabrts.github.io/main/FRED_data_MSPUS.json",
"format": {
  "property": "observations"}
    },

  "height": 300,
  "width": 300,
  "mark": {
    "type": "line",
    "point": false,
    "interpolate": "monotone",
    "color": "#3333FF"
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {
        "title": null,
        "labelSeparation": 20,
        "ticks": false,
        "grid": true
      }
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "title": "USD",
      "axis": {"grid": true}
    }
  }
}
