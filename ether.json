 {
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

  "description": "Etherium Price",

   "title": {
    "text": "Ether: this year",
    "subtitle":"High-low trading range, 12h-time periods. Source: coinapi.io",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start"},
  
   
  "data": {
      "url": "https://rest.coinapi.io/v1/ohlcv/ETH/USD/history?period_id=12HRS&time_start=2021-01-01T00:00:00&limit=100&include_empty_items=false&apikey= 90ECCE2B-5A53-4DE9-80E2-7D1490A49275",
      "format": {"type": "json"}},
  

  "height": 300,
  "width": 300,
  

  "mark": {
    "type": "area",
    "point": false,
    "color":"#f4c245"
  },

  
  "encoding": {
    
    "x": {
      "field": "time_period_start",
      "type": "temporal",
      "axis": {
        "title": null,
        "grid": false,
        "labelSeparation": 10}},

    "y": {
      "field": "price_low",
      "type": "quantitative",
      "title": "",
      "scale":{"domain":[30000, 60000]},
      "axis": {
        "grid": false,
        "format":"$.2s"}},

    "y2": {"field": "price_high"}       
  }
}
